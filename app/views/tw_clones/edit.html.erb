<html>
  <head>
    <meta charset="UTF-8">
    <title>twitter_clone</title>
  </head>
  <body>
    <article>
      <div class = "main">
        <div id="index">
          <div class = "wrapper">
            <ul class="new">
            <p>編集する</p>
              <%= form_with(model: @twc, local: true, url: tw_clone_path(@twc.id) ) do | form| %>
                <div><%= form.text_area :content %></div>
                <%= form.submit('編集する') %>
              <% end %>
            </ul>
          </div>
        </div>
      </div>
    </article>
  </body>
</html>